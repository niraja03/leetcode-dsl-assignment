class Solution {
public:
    int dominantIndex(vector<int>& nums) {
        int n = nums.size() ;
        int j =0;
        for( int i = 0 ; i<n ; i++) {
            if(nums[i]>nums[j]) {
                j=i;
            }
        }
        for(int i=0; i<n ; i++) {
            if (i != j && nums[j] < 2 * nums[i]) {
                return -1;
            }
        }
        return j;
    }
};
