class Solution {
public:
    vector<int> intersection(vector<int>& nums1, vector<int>& nums2) {
        unordered_set<int> s(nums1.begin(), nums1.end());
        vector<int> result;
        
        for (int x : nums2) {
            if (s.erase(x)) {         // if x was in s, erase it and add to result
                result.push_back(x);
            }
        }
        
        return result;
    }
};
